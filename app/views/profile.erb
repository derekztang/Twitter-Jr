<div class="container">
  <div class="bio">
    <h1 class="username"><%= @user.username %></h1>
    <p>Bio: <%= @user.user_bio %></p>
    <img src="<%=@user.user_bio_picture%>">
  </div>
  <div>
    <% @tweets.each do |tweet| %>
    <div class="tweet_username">
      <div class="tweet"><%= tweet.content %></div>
    </div>
      <% end %>
</div>
