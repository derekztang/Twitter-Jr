<div class="container">
  <h1 id="feed_title"> Your feed </h1>
  <form class="enter_tweet" action='/tweets' method='post'>
    <input class="enter" type='text' name='content'>
    <input type='submit' value='tweet'>
  </form>

  <div class = "followee">
    <% @followee_tweets.each do |tweet| %>
      <div class="tweet_username">
        <div class="left"><a href="/users/<%= tweet.user.id %>"><%= tweet.user.username %></a></div>
        <div class="right"><%= tweet.content %></div>
        <div class="clear"></div>
      </div>
    <% end %>
  </div>
</div>
